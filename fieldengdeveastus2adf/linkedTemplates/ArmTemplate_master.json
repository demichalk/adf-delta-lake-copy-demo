{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "fieldengdeveastus2adf"
		},
		"fieldengdeveastus2adb_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'fieldengdeveastus2adb'"
		},
		"fieldengdeveastus2adls_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'fieldengdeveastus2adls'"
		},
		"fieldengdeveastus2sa_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'fieldengdeveastus2sa'"
		},
		"fieldengdeveastus2sqlsrvadventureworks_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'fieldengdeveastus2sqlsrvadventureworks'"
		},
		"adfcopydemosa_properties_typeProperties_serviceEndpoint": {
			"type": "string",
			"defaultValue": "https://fieldengdeveastus2sa.blob.core.windows.net"
		},
		"fieldengdeveastus2adls_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://fieldengdeveastus2adls.dfs.core.windows.net"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "fieldengdeveastus2adf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"fieldengdeveastus2adb_accessToken": {
						"value": "[parameters('fieldengdeveastus2adb_accessToken')]"
					},
					"fieldengdeveastus2adls_accountKey": {
						"value": "[parameters('fieldengdeveastus2adls_accountKey')]"
					},
					"fieldengdeveastus2sa_connectionString": {
						"value": "[parameters('fieldengdeveastus2sa_connectionString')]"
					},
					"fieldengdeveastus2sqlsrvadventureworks_connectionString": {
						"value": "[parameters('fieldengdeveastus2sqlsrvadventureworks_connectionString')]"
					},
					"adfcopydemosa_properties_typeProperties_serviceEndpoint": {
						"value": "[parameters('adfcopydemosa_properties_typeProperties_serviceEndpoint')]"
					},
					"fieldengdeveastus2adls_properties_typeProperties_url": {
						"value": "[parameters('fieldengdeveastus2adls_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}